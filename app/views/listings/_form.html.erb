<%= form_for @listing do |f| %>
  <%= f.label :street %>
  <%= f.text_field :street %><br>
  <%= f.label :city%>
  <%= f.text_field :city %><br>
  <%= f.label :state %>
  <%= f.text_field :state, placeholder: 'example NJ' %><br>
  <%= f.label :zip_code %>
  <%= f.text_field :zip_code %><br><br>

  <%= f.label :property_type %>
  <%= f.text_field :property_type %><br>
  <%= f.label :bedrooms %>
  <%= f.number_field :bedrooms %><br>
  <%= f.label :bathrooms %>
  <%= f.number_field :bathrooms %><br><br>

  <%= f.label :list_price %>
  $ <%= f.text_field :list_price %><br><br>

  <%= f.hidden_field :user_id, :value => session[:user_id] %>
  <%= f.submit %>
<% end %>